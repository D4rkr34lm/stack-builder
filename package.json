{
  "name": "stack-builder",
  "version": "1.0.0",
  "description": "The monorepo of stack-builder",
  "workspaces": [
    "./packages/backend",
    "./packages/frontend",
    "./packages/shared",
    "./packages/api"
  ],
  "author": "Manuel Frohn",
  "license": "ISC",
  "private": true,
  "type": "module",
  "scripts": {
    "shared:watch": "cd ./packages/shared && npm run watch",
    "shared:build": "cd ./packages/shared && npm run build",
    "api:build": "cd ./packages/api && npm run build",
    "backend:build": "cd ./packages/backend && npm run build",
    "backend:watch": "cd ./packages/backend && npm run watch",
    "backend:test": "cd ./packages/backend && npm run test",
    "backend:dev": "cd ./packages/backend && npm run dev"
  },
  "devDependencies": {
    "@eslint/js": "9.14.0",
    "@types/lodash": "^4.17.13",
    "@types/luxon": "^3.4.2",
    "eslint": "9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.12.0",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "typescript-eslint": "8.13.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "neverthrow": "^8.1.1"
  }
}
