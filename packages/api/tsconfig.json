{
  "compilerOptions": {
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "./lib",
    "declaration": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "module": "ESNext",
    "target": "ES6",
    "moduleResolution": "Node"
  },
  "include": ["./gen/**/*"]
}